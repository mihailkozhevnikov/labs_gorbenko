#include <iostream>

using namespace std;
void main()
{
  int n;
	cout<<"vvedite n"<<endl;
	cin>>n;
	double *a = new double[n];
	for(int i=0;i<=n;i++)
	{
		cin>>a[i];
	}
	int k=-1;
	for(int i=0;i<=n;i++)
	{
		if (a[i]<0)
		{
			k+=1;
			int t= a[i];
			for (int j=k+1;j<=i;j++)
			{
				a[j]=a[j-1];
			}
			a[k]=t;
		}
	}

	for(int i=0;i<=n;i++)
	{
		cout<<a[i]<<" ";
	}
	system("pause");
}

#include <iostream>

using namespace std;
void main()
{
  int n;
	cout<<"vvedite n"<<endl;
	cin>>n;
	int *a=new int[n];
	for (int i=0;i<=n;i++)
	{
		cin>>a[i];
	}
	for (int i=0;i<=n;i++)
	{
		for(int j=0;j<=i;j++)
		{
			if (i==j*j)
			{
				cout<<a[i]<<" ";
				break;
			}
		}
	}
	system("pause");
}
